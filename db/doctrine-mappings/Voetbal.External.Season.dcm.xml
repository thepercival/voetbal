<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Voetbal\External\Season" table="externalseasons" repository-class="Voetbal\External\Season\Repository">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="externalId" column="externalId" type="string" length="100" nullable="false" unique="false"/>
    <many-to-one field="externalSystem" target-entity="Voetbal\External\System">
      <join-columns>
        <join-column name="externalSystemid" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="importableObject" target-entity="Voetbal\Season" inversed-by="externals">
      <join-columns>
        <join-column name="importableObjectid" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
    <unique-constraints>
      <unique-constraint columns="externalSystemid,externalId" name="main_idx" />
    </unique-constraints>
  </entity>
</doctrine-mapping>