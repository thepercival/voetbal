# config/db.dcm.yml
Voetbal\DAO\Season:
  type: entity
  table: seasons
  id:
    id:
      type: integer
      generator:
        strategy: AUTO
  fields:
    name: { type: string, length : 9 }
  embedded:
    period:
      class: Voetbal\DAO\Period
      columnPrefix: false
    externid:
      class: Voetbal\DAO\External
      columnPrefix: false