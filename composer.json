{
  "name": "thepercival/voetbal",
  "description": "library  for football seasons, leagues, competitions and more too come",
  "keywords": ["voetbal","football"],
  "homepage": "https://www.superelf-eredivisie.nl/",
  "license": "GPL-2.0",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Coen Dunnink",
      "email": "coendunnink@gmail.com"
    }
  ],
  "support": {
	"email": "coendunnink@gmail.com"    
  },
  "require": {
    "vlucas/phpdotenv": "^2.4",
    "nesbot/carbon": "~1.21",
    "league/period": "~3.1",
    "league/tactician": "~1.0",
    "doctrine/orm": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Voetbal\\": "domain/",
      "VoetbalRepository\\": "db/repositories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Voetbal\\Tests\\": "tests/"
    },
    "psr-0": {
      "": "tests/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": [
      "phpunit -c tests/config.xml"
    ]
  }
}
