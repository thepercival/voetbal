{
  "name": "thepercival/voetbal",
  "description": "library  for football competitions and more",
  "keywords": ["voetbal","football"],
  "homepage": "https://github.com/thepercival/voetbal",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Coen Dunnink",
      "email": "coendunnink@gmail.com"
    }
  ],
  "support": {
	"email": "coendunnink@gmail.com"    
  },
  "require": {
    "php": ">=7.1",
    "vlucas/phpdotenv": "^2.4",
    "nesbot/carbon": "~1.21",
    "league/period": "~3.3",
    "league/tactician": "~1.0",
    "doctrine/orm": "^2.5",
    "jms/serializer": "^1.5",
    "symfony/yaml": "^3.2",
    "monolog/monolog": "^1.17"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Voetbal\\": "domain/",
      "Voetbal\\Action\\": "actions/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Voetbal\\Tests\\": "tests/"
    },
    "psr-0": {
      "": "tests/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": [
      "vendor\\bin\\phpunit -c tests/config.xml"
    ]
  }
}
