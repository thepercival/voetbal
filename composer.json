{
  "name": "thepercival/voetbal",
  "description": "library  for football competitions and more",
  "keywords": ["voetbal","football"],
  "homepage": "https://github.com/thepercival/voetbal",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Coen Dunnink",
      "email": "coendunnink@gmail.com"
    }
  ],
  "support": {
	"email": "coendunnink@gmail.com"    
  },
  "require": {
    "php": ">=7.2",
    "vlucas/phpdotenv": "^2.5",
    "league/period": "^4.1",
    "doctrine/orm": "^2.6",
    "jms/serializer": "^2.0",
    "symfony/yaml": "^4.2",
    "monolog/monolog": "^1.24",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Voetbal\\": "domain/",
      "Voetbal\\Action\\": "actions/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Voetbal\\Tests\\": "tests/cases"
    },
    "psr-0": {
      "": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit --bootstrap vendor/autoload.php tests/cases/"
    ]
  }
}
